class MyNameFor
{
	public static void main(String[] args) throws Exception
	{
		int n=10;
		for(int i=0;i<n;i++)
		{
			for(int j=0;j<n;j++)
			{
				//A
				if(j==0 || i==0 || j==n-1 || i==n/2)
					System.out.print("A ");
				else
					System.out.print("  ");
				Thread.sleep(50);
			}
			System.out.print(" ");
			
			for(int j=0;j<n;j++)
			{
				//k
				if(j==0 || i+j==n-1-n/2||i==j+n/2)
					System.out.print("K ");
				else
					System.out.print("  ");
				Thread.sleep(50);
			}
			
			
			
			for(int j=0;j<n;j++)
			{
				//A
				if(j==0 || i==0 || j==n-1 || i==n/2)
					System.out.print("A ");
				else
					System.out.print("  ");
				Thread.sleep(50);
			}
			System.out.print(" ");
			
			
			for(int j=0;j<n;j++)
			{
				//S
				if(i==0||i==n/2||j==n-1 && i>=n/2||i==n-1||j==0 && i<=n/2)
					System.out.print("S ");
				else
					System.out.print("  ");
				Thread.sleep(50);
			}
			System.out.print(" ");
			
			
			for(int j=0;j<n;j++)
			{
				//H
				if(j==0 || i==n/2 || j==n-1)
					System.out.print("H ");
				else
					System.out.print("  ");
				Thread.sleep(50);
			}
			System.out.print(" ");
			
			
			
			System.out.println();
		}
			
	}
}
